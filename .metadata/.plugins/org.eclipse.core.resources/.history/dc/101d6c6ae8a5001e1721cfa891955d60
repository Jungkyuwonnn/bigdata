package 삼항연산자;

import java.util.Scanner;

public class Ex03농구공예제 {

	public static void main(String[] args) {

		
		Scanner sc = new Scanner(System.in);
		System.out.print("농구공의 개수를 입력하시오: ");
		int num1 = sc.nextInt();
		boolean num2 = num1 % 5 ==0;
		int num3 = num1 % 5 == 0? num3 : num1 + 1;
		System.out.print("필요한 상자의 개수: " + num3);
		
		
		
		
		
		
		
		
		
		
	}

}
